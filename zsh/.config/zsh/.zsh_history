#!/bin/sh\
\
# Get full list of entries\
entries=$(pass ls | sed '1d' | tr -d ' ' | sed '/^$/d')\
\
# Build a menu: basename  full path\
menu=$(for f in $entries; do\
    printf "%s\t%s\n" "$(basename "$f")" "$f"\
done)\
\
# Let user choose by name\
choice=$(printf "%s\n" "$menu" | dmenu -i -l 20 -p "Choose password:" | cut -f2)\
\
# Exit if nothing chosen\
[ -z "$choice" ] && exit 1\
\
# Copy or show the password\
pass show "$choice" | xclip -selection clipboard\
echo "Copied $choice"\

