#!/bin/sh\
\
# Get all actual pass entries (files ending with .gpg)\
entries=$(find ~/.password-store -type f -name '*.gpg')\
\
# Build menu with basenames, map to full path\
choice=$(printf '%s\n' "$entries" \\
    | sed 's#^.*/##; s/\.gpg$//' \\
    | dmenu -l 20 -p "Choose entry:")\
\
# Exit if nothing chosen\
[ -z "$choice" ] && exit 1\
\
# Map choice back to full path\
selected=$(printf '%s\n' "$entries" \\
    | grep "/$choice.gpg$")\
\
# Convert file path  pass store key\
key=${selected#"$HOME/.password-store/"}\
key=${key%.gpg}\
\
# Show the actual password\
pass show "$key"\

